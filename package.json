{
  "name": "jepser-static",
  "version": "1.0.0",
  "description": "Static website",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "deploy-now": "now -e NODE_ENV=production --token $NOW_TOKEN",
    "alias": "now alias --token=$NOW_TOKEN"
  },
  "now": {
    "name": "jepser",
    "alias": "jepser.com",
    "target": "out"
  },
  "author": "Jepser",
  "license": "ISC",
  "dependencies": {
    "@typeform/embed": "^0.4.2",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-styled-components": "^1.3.0",
    "contentful": "5.0.5",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.19.1",
    "next": "^4.1.4",
    "raw-loader": "^0.5.1",
    "react": "16",
    "react-dom": "16",
    "react-markdown": "^3.1.3",
    "react-prism": "^4.3.1",
    "styled-components": "^2.2.3"
  },
  "devDependencies": {
    "now": "^9.0.1"
  }
}
